{
  "name": "11ty-boilerplate",
  "version": "0.0.4",
  "description": "A quickstart template for rapidly putting up website with 11ty and Gulp.",
  "main": "eleventy.js",
  "author": "Pavel Sanchez",
  "license": "ISC",
  "engines": {
    "node": "14.15.4"
  },
  "repository": {
    "url": "https://github.com/PaleBluDot/quickstart.git"
  },
  "scripts": {
    "start": "cross-env ELEVENTY_ENV=dev concurrently \"npm run dev\" \"gulp dev\"",
    "assets": "gulp build",
    "dev": "eleventy --serve",
    "clean": "rm -r build",
    "prettier": "npx prettier --write .",
    "build": "cross-env ELEVENTY_ENV=dev eleventy && npm run assets"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "axios": "^0.19.2",
    "browser-sync": "^2.26.3",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "html-minifier": "^4.0.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "pump": "^3.0.0"
  },
  "dependencies": {}
}
