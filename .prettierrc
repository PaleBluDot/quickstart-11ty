trailingComma: "es5"
printWidth: 120
useTabs: true
tabWidth: 2
semi: true
singleQuote: false
jsxSingleQuote: true
jsxBracketSameLine: true
bracketSpacing: true
overrides:
  - files:
      - "*.md"
      - "*.yaml"
    options:
      useTabs: false
      tabWidth: 2
  - files: "*.html"
    options:
      printWidth: 400
  - files: "*.js"
    options:
      singleQuote: true
